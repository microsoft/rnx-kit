{
  "private": true,
  "name": "@rnx-kit/docsite",
  "version": "0.0.0",
  "scripts": {
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "format": "prettier --write --log-level error $(git ls-files '*.[jt]s' '*.md' '*.mdx' '*.tsx')",
    "serve": "docusaurus serve",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "typecheck": "tsc",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "dependencies": {
    "@docusaurus/core": "^3.5.0",
    "@docusaurus/preset-classic": "^3.5.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "docusaurus-lunr-search": "^3.5.0",
    "prism-react-renderer": "^2.3.0",
    "react": "~19.1.1",
    "react-dom": "~19.1.1"
  },
  "devDependencies": {
    "@tsconfig/docusaurus": "^2.0.1",
    "@types/react": "~19.2.0",
    "prettier": "catalog:",
    "prettier-plugin-organize-imports": "^4.0.0",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=18.12"
  },
  "packageManager": "yarn@4.10.3",
  "resolutions": {
    "@docsearch/react": "link:../incubator/ignore",
    "@pnpm/network.ca-file/graceful-fs": "^4.2.10",
    "algoliasearch": "link:../incubator/ignore",
    "algoliasearch-helper": "link:../incubator/ignore"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "../.github/prettierrc.json"
}
