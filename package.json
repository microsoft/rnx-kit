{
  "name": "rnx-kit",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "Jason Morse <jasonmo@microsoft.com>",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/rnx-kit.git"
  },
  "scripts": {
    "build": "lage build",
    "beachball:publish": "beachball publish",
    "change": "beachball change",
    "ci": "yarn --cache-folder .yarn-offline-mirror --prefer-offline --frozen-lockfile --non-interactive",
    "check": "beachball check",
    "fast": "node scripts/tasks/fast-install.js",
    "test": "lage test --passWithNoTests"
  },
  "devDependencies": {
    "beachball": "^1.49.0",
    "lage": "^0.27.0"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "scripts"
    ]
  },
  "lage": {
    "npmClient": "yarn",
    "pipeline": {
      "build": [
        "^build"
      ]
    }
  },
  "beachball": {
    "branch": "origin/main",
    "disallowedChangeTypes": [
      "major"
    ],
    "access": "public"
  }
}
