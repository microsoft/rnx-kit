{
  "private": true,
  "name": "kit-test-configured",
  "version": "0.0.1",
  "description": "Package with rnx-kit configuration",
  "rnx-kit": {
    "bundle": {
      "id": "core",
      "entryFile": "./core-entry.js",
      "bundleOutput": "./app.bundle",
      "targets": [
        "ios",
        "android",
        "macos",
        "windows"
      ],
      "platforms": {
        "android": {
          "assetsDest": "./build-out/res"
        }
      }
    }
  }
}
