    "@rnx-kit/tools-apple": "^0.1.2",
    "@rnx-kit/tools-language": "^2.0.0",
    "@rnx-kit/tools-node": "^2.1.1",
    "@rnx-kit/tools-react-native": "^1.4.2",
    "commander": "^11.1.0",
    "ora": "^5.4.1",
    "qrcode": "^1.5.0"
  },
  "peerDependencies": {
    "jest": ">=26.0",
    "react-native": ">=0.64"
  },
