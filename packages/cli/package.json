{
  "name": "@rnx-kit/cli",
  "version": "0.15.2",
  "description": "Command-line interface for working with kit packages in your repo",
  "homepage": "https://github.com/microsoft/rnx-kit/tree/main/packages/cli",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/rnx-kit",
    "directory": "packages/cli"
  },
  "scripts": {
    "build": "rnx-kit-scripts build",
    "depcheck": "rnx-kit-scripts depcheck",
    "format": "rnx-kit-scripts format",
    "lint": "rnx-kit-scripts lint",
    "test": "rnx-kit-scripts test"
  },
  "dependencies": {
    "@rnx-kit/align-deps": "^2.0.1",
    "@rnx-kit/config": "^0.5.2",
    "@rnx-kit/console": "^1.0.11",
    "@rnx-kit/metro-plugin-cyclic-dependencies-detector": "^1.0.21",
    "@rnx-kit/metro-plugin-duplicates-checker": "^2.0.0",
    "@rnx-kit/metro-plugin-typescript": "^0.1.0",
    "@rnx-kit/metro-serializer": "^1.0.11",
    "@rnx-kit/metro-serializer-esbuild": "^0.1.0",
    "@rnx-kit/metro-service": "^3.0.2",
    "@rnx-kit/third-party-notices": "^1.2.13",
    "@rnx-kit/tools-language": "^1.4.1",
    "@rnx-kit/tools-node": "^1.3.1",
    "@rnx-kit/tools-react-native": "^1.2.3",
    "chalk": "^4.1.0",
    "find-up": "^5.0.0",
    "fs-extra": "^10.0.0",
    "ora": "^5.4.1",
    "qrcode": "^1.5.0",
    "readline": "^1.3.0"
  },
  "peerDependencies": {
    "@react-native-community/cli-server-api": "^5.0.0-0 || ^6.0.0-0",
    "jest-cli": "^26.0 || ^27.0"
  },
  "peerDependenciesMeta": {
    "@react-native-community/cli-server-api": {
      "optional": true
    },
    "jest-cli": {
      "optional": true
    }
  },
  "devDependencies": {
    "@react-native-community/cli-types": "^6.0.0",
    "@rnx-kit/scripts": "*",
    "@types/metro": "^0.66.0",
    "@types/metro-babel-transformer": "^0.66.0",
    "@types/metro-config": "^0.66.0",
    "@types/qrcode": "^1.4.2",
    "jest": "^27.0.0",
    "jest-extended": "^2.0.0",
    "memfs": "^3.4.1",
    "metro-config": "^0.67.0"
  },
  "depcheck": {
    "ignoreMatches": [
      "@react-native-community/cli-clean",
      "metro",
      "metro-babel-transformer",
      "readline"
    ]
  },
  "eslintConfig": {
    "extends": "@rnx-kit/eslint-config"
  },
  "jest": {
    "preset": "@rnx-kit/scripts",
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ]
  }
}
