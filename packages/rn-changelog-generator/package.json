{
  "name": "@rnx-kit/rn-changelog-generator",
  "version": "0.0.1",
  "description": "New package called rn-changelog-generator",
  "homepage": "https://github.com/microsoft/rnx-kit/tree/main/packages/rn-changelog-generator#readme",
  "license": "MIT",
  "files": [
    "lib/**/*.d.ts",
    "lib/changelog-generator.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "rn-changelog-generator": "lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/rnx-kit",
    "directory": "packages/rn-changelog-generator"
  },
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "build": "rnx-kit-scripts build && rnx-kit-scripts bundle",
    "format": "rnx-kit-scripts format",
    "generate": "env TS_NODE_TRANSPILE_ONLY=true node $_NODE_OPTS -r ts-node/register ./lib/changelog-generator.js",
    "generate:debug": "env _NODE_OPTS=--inspect-brk yarn -s generate",
    "lint": "rnx-kit-scripts lint",
    "test": "rnx-kit-scripts test"
  },
  "devDependencies": {
    "@octokit/rest": "^18.5.3",
    "@rnx-kit/scripts": "*",
    "@types/fast-levenshtein": "0.0.2",
    "chalk": "^4.1.0",
    "deepmerge": "^4.2.2",
    "p-limit": "^3.1.0",
    "fast-levenshtein": "^3.0.0",
    "simple-git": "^2.39.0",
    "ts-node": "^10.4.0",
    "yargs": "^16.0.0"
  },
  "eslintConfig": {
    "extends": "@rnx-kit/eslint-config"
  },
  "jest": {
    "preset": "@rnx-kit/scripts"
  }
}
