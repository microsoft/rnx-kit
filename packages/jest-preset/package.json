{
  "name": "@rnx-kit/jest-preset",
  "version": "0.1.16",
  "description": "Jest preset for React Native code bases",
  "homepage": "https://github.com/microsoft/rnx-kit/tree/main/packages/jest-preset#readme",
  "license": "MIT",
  "files": [
    "jest-preset.js",
    "src/*.js"
  ],
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/rnx-kit",
    "directory": "packages/jest-preset"
  },
  "scripts": {
    "build": "rnx-kit-scripts build",
    "format": "rnx-kit-scripts format",
    "lint": "rnx-kit-scripts lint",
    "test": "rnx-kit-scripts test"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "find-up": "^5.0.0"
  },
  "peerDependencies": {
    "react-native": "^0.0.0-0 || >=0.63"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^8.33.0",
    "@jest/types": "^29.2.1",
    "@react-native-community/cli-types": "^12.3.0",
    "@rnx-kit/scripts": "*",
    "@rnx-kit/tsconfig": "*",
    "@types/jest": "^29.2.1",
    "@types/node": "^20.0.0",
    "eslint": "^8.23.0",
    "jest": "^29.2.1",
    "prettier": "^3.0.0",
    "react": "18.2.0",
    "react-native": "^0.73.0",
    "typescript": "^5.0.0"
  },
  "depcheck": {
    "ignoreMatches": [
      "@babel/core",
      "@babel/preset-env",
      "@babel/preset-typescript",
      "@jest/types",
      "@react-native-community/cli-types",
      "react"
    ]
  },
  "jest": {
    "preset": "./private/jest-preset.js"
